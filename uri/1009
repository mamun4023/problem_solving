#include<bits/stdc++.h>
#include <iostream>
#include <iomanip>
using namespace std;



int main(){

    string name;
    double fix_salary, total_sales, total_salary;

    cin>>name;
    cin>>fix_salary;
    cin>>total_sales;

    total_salary = ( (15 * total_sales)/100) + fix_salary;

    printf("TOTAL = R$ %.2lf\n",total_salary);



return 0;
}

